{
  "name": "start9-companion",
  "version": "2.0.0",
  "description": "Companion app for setting up Start9 devices",
  "author": "Start9 Labs",
  "private": true,
  "homepage": "https://start9labs.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run postinstall && ionic build && npx cap sync",
    "ios": "npm run build && npx cap open ios",
    "android": "npm run build && npx cap open android",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node patch_polyfills.js && cp -r node_modules/crypto-browserify node_modules/crypto && cp -r node_modules/stream-browserify node_modules/stream && cp -r node_modules/vm-browserify node_modules/vm"
  },
  "dependencies": {
    "@angular/common": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/forms": "^10.0.0",
    "@angular/platform-browser": "^10.0.0",
    "@angular/platform-browser-dynamic": "^10.0.0",
    "@angular/router": "^10.0.0",
    "@capacitor-community/http": "^0.2.1",
    "@capacitor/core": "^2.2.0",
    "@ionic-native/core": "^5.25.0",
    "@ionic-native/zeroconf": "^5.25.0",
    "@ionic/angular": "^5.2.0",
    "async-mutex": "^0.2.0",
    "base32.js": "^0.1.0",
    "buffer": "^5.6.0",
    "cordova-plugin-zeroconf": "^1.4.0",
    "core-js": "^3.4.0",
    "crypto": "^1.0.1",
    "crypto-js": "^4.0.0",
    "hi-base32": "^0.5.0",
    "rxjs": "^6.5.0",
    "zone.js": "^0.10.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.0",
    "@angular/cli": "^10.0.0",
    "@angular/compiler": "^10.0.0",
    "@angular/compiler-cli": "^10.0.0",
    "@capacitor/android": "^2.2.0",
    "@capacitor/cli": "^2.2.0",
    "@capacitor/ios": "^2.2.0",
    "@types/crypto-js": "^3.1.0",
    "@types/node": "^14.0.0",
    "codelyzer": "^6.0.0",
    "ts-node": "^8.10.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-zeroconf": {}
    }
  }
}
