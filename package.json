{
  "name": "setup-app",
  "version": "1.1.2",
  "description": "Mobile app for setting up the Start9 Embassy",
  "author": "Start9 Labs, Inc",
  "private": true,
  "homepage": "https://start9labs.com",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run postinstall && ionic build && npx cap sync",
    "ios": "npm run build && npx cap open ios",
    "android": "npm run build && npx cap open android",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "node patch_polyfills.js && cp -r node_modules/crypto-browserify node_modules/crypto && cp -r node_modules/stream-browserify node_modules/stream && cp -r node_modules/vm-browserify node_modules/vm"
  },
  "dependencies": {
    "@angular/common": "^10.1.4",
    "@angular/core": "^10.1.4",
    "@angular/forms": "^10.1.4",
    "@angular/platform-browser": "^10.1.4",
    "@angular/platform-browser-dynamic": "^10.1.4",
    "@angular/router": "^10.1.4",
    "@capacitor/core": "^2.4.2",
    "@ionic-native/core": "^5.25.0",
    "@ionic-native/zeroconf": "^5.25.0",
    "@ionic/angular": "^5.3.4",
    "@start9labs/capacitor-http": "0.1.2",
    "async-mutex": "^0.2.0",
    "base32.js": "^0.1.0",
    "big-integer": "^1.6.48",
    "buffer": "^5.6.0",
    "capacitor-cert-installer": "file:../capacitor-cert-installer",
    "cordova-plugin-zeroconf": "^1.4.0",
    "core-js": "^3.4.0",
    "crypto-js": "^4.0.0",
    "elliptic": "^6.5.3",
    "js-sha3": "^0.8.0",
    "node-forge": "^0.10.0",
    "openssl-nodejs": "^1.0.5",
    "rxjs": "^6.5.0",
    "zone.js": "^0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.4",
    "@angular/cli": "^10.1.4",
    "@angular/compiler": "^10.1.4",
    "@angular/compiler-cli": "^10.1.4",
    "@capacitor/android": "^2.4.2",
    "@capacitor/cli": "^2.4.2",
    "@capacitor/ios": "^2.4.2",
    "@types/crypto-js": "^3.1.0",
    "@types/node": "^14.11.2",
    "codelyzer": "^6.0.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.2",
    "typescript": "^4.0.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-zeroconf": {}
    }
  }
}
